{
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "eject": "expo eject",
        "typecheck": "tsc",
        "format:check": "npx prettier --check \"**/*.{js,jsx,json,ts,tsx}\"",
        "format": "npx prettier --write \"**/*.{js,jsx,json,ts,tsx}\"",
        "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\""
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run typecheck && lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": "eslint",
        "**/*.{js,jsx,json,ts,tsx}": "prettier --write"
    },
    "dependencies": {
        "@expo-google-fonts/delius-unicase": "^0.1.0",
        "@react-native-community/async-storage": "~1.12.0",
        "@react-native-community/masked-view": "0.1.10",
        "@react-native-community/netinfo": "5.9.7",
        "@react-navigation/native": "^5.7.6",
        "@react-navigation/stack": "^5.9.3",
        "@twotalltotems/react-native-otp-input": "^1.3.11",
        "aws-amplify": "^3.3.11",
        "aws-amplify-react-native": "^4.2.9",
        "expo": "^40.0.0",
        "expo-app-loading": "^1.0.1",
        "expo-av": "~8.7.0",
        "expo-font": "~8.4.0",
        "expo-haptics": "~8.4.0",
        "expo-linear-gradient": "~8.4.0",
        "expo-notifications": "~0.8.2",
        "expo-status-bar": "~1.0.3",
        "graphql-tag": "^2.11.0",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
        "react-native-gesture-handler": "~1.8.0",
        "react-native-modal": "^11.6.1",
        "react-native-reanimated": "~1.13.0",
        "react-native-safe-area-context": "3.1.9",
        "react-native-screens": "~2.15.2",
        "react-native-web": "~0.13.12",
        "zen-observable": "^0.8.15",
        "expo-ads-admob": "~8.4.0"
    },
    "devDependencies": {
        "@types/react": "~16.9.35",
        "@types/react-dom": "~16.9.8",
        "@types/react-native": "~0.63.2",
        "@types/zen-observable": "^0.8.2",
        "@typescript-eslint/eslint-plugin": "^4.4.0",
        "@typescript-eslint/parser": "^4.4.0",
        "babel-plugin-module-resolver": "^4.0.0",
        "eslint": "^7.10.0",
        "eslint-config-prettier": "^6.12.0",
        "eslint-plugin-react": "^7.21.3",
        "husky": "^4.3.0",
        "lint-staged": "^10.4.0",
        "prettier": "2.1.2",
        "react-native-web": "~0.13.12",
        "typescript": "~4.0.0"
    },
    "private": true
}
